@font-face {
  font-family: 'Open-Sans-Light';
  src: url('<%= asset_path("Open-Sans-Light.ttf.woff") %>') format('woff'),
      url('<%= asset_path("Open-Sans-Light.ttf.svg") + "#Open-Sans-Light" %>') format('svg'),
      url('<%= asset_path("Open-Sans-Light.ttf.eot") %>'),
      url('<%= asset_path("Open-Sans-Light.ttf.eot") + "?#iefix" %>') format('embedded-opentype');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: 'icomoon';
  src:url('<%= asset_path("icomoon-webfont.eot") + "?ipptj7" %>');
  src:url('<%= asset_path("icomoon-webfont.eot") + "?ipptj7#iefix" %>') format('embedded-opentype'),
      url('<%= asset_path("icomoon-webfont.ttf") + "?ipptj7" %>') format('truetype'),
      url('<%= asset_path("icomoon-webfont.woff") + "?ipptj7" %>') format('woff'),
      url('<%= asset_path("icomoon-webfont.svg") + "?ipptj7#icomoon" %>') format('svg');
  font-weight: normal;
  font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
  font-family: 'icomoon';
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-icon-01:before {
  content: "\e800";
}
.icon-icon-02:before {
